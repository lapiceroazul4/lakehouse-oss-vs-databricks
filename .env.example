# MinIO Configuration - Servicio de almacenamiento de objetos
MINIO_ROOT_USER=admin
MINIO_ROOT_PASSWORD=password
MINIO_ACCESS_KEY=admin
MINIO_SECRET_KEY=password
MINIO_ENDPOINT_URL=http://minio:9000
MINIO_REGION=us-east-1

# Nessie Configuration - Catálogo de datos para Apache Iceberg
NESSIE_URI=http://nessie:19120/iceberg/main
NESSIE_ACCESS_KEY=admin
NESSIE_SECRET_KEY=password

# ClickHouse Configuration - Base de datos analítica
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=password
CLICKHOUSE_HOST=clickhouse
CLICKHOUSE_PORT=9000
CLICKHOUSE_HTTP_PORT=8123
CLICKHOUSE_DATABASE=default

# Azure Storage Configuration - Almacenamiento en la nube de Microsoft
AZURE_STORAGE_ACCOUNT_NAME=your_storage_account_name
AZURE_STORAGE_ACCOUNT_KEY=your_azure_storage_key
AZURE_CONTAINER_NAME=your_container_name
AZURE_BUCKET_URL=abfss://your-container@your-account.dfs.core.windows.net/your-path

# Jupyter Configuration - Configuración del servidor Jupyter
JUPYTER_TOKEN=
JUPYTER_PASSWORD=
